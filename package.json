{
  "name": "react-app-in-twixt-ui",
  "homepage": "https://web-slate.github.io/TwixtUI-React-App/",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production --progress",
    "clean": "rm -rf node_modules",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d public",
    "get-ui": "cd src && git submodule add https://github.com/web-slate/TwixtUI.git",
    "clear-ui": "rm -rf TwixtUI && rm -f .gitmodules",
    "clear-cache-ui": "git rm --cached TwixtUI -f || true && rm -rf .git/modules/TwixtUI || true",
    "commit-clear": "git add . && git commit -am \"tech(ui): remove TwixtUI submodule\"",
    "ui": "npm run clear-ui && npm run clear-cache-ui && npm run commit-clear && npm run get-ui"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@svgr/webpack": "^8.1.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.1.1",
    "html-webpack-plugin": "^5.6.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "private": true
}
